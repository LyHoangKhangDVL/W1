                    NOIDUNG = f"\033[1;97m: {nd} \033[1;31m| \033[1;35mNhây Thành Công"
                print(f"{NOIDUNG}")
                idelay(delay)
        except Exception as e:
          print(f"{do}Lỗi bị ngắt kết lối vui lòng kết lối lại mạng để tiếp tục nhây")
          time.sleep(5)
else:
    print(f"{do}Vui lòng chọn đúng")  
